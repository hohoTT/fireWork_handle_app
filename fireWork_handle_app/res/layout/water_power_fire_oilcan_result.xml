<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="center_vertical"
    android:orientation="vertical" >
    
    <LinearLayout 
        
        android:id="@+id/water_power_fire_oilcan_result_65"
	    android:layout_width="match_parent"
	    android:layout_height="wrap_content"
	    android:layout_margin="40dp"
	    android:orientation="horizontal">
	    
        <TextView 
        android:id="@+id/text_water_power_fire_oilcan_65"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" 
        android:text="@string/text_water_power_fire_oilcan_65" />
        
        <TextView 
        android:id="@+id/text_water_power_fire_oilcan_65_result"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
        
    </LinearLayout>
    
    <LinearLayout 
        
        android:id="@+id/water_power_fire_oilcan_result_80"
	    android:layout_width="match_parent"
	    android:layout_height="wrap_content"
	    android:layout_margin="40dp"
	    android:orientation="horizontal">
	    
        <TextView 
        android:id="@+id/text_water_power_fire_oilcan_80"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" 
        android:text="@string/text_water_power_fire_oilcan_80" />
        
        <TextView 
        android:id="@+id/text_water_power_fire_oilcan_80_result"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
        
    </LinearLayout>
    
</LinearLayout>
